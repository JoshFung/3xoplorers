--cutscene functions
function icutscene()
 parts={}
 starfield={}
 startimer=0

 text1={
  "welcome to minigame 1",
  "press ➡️ to continue"
 }
 
 text2={
  "welcome to minigame 2",
  "press ➡️ to continue"
 }
 
 msgindex=1
end

function ucutscene()
 startimer+=1
 if startimer>2 then
 
	 add(starfield,{
		 x=rnd(128),
		 y=-10,
		 c=7,
		 speed=.3+rnd(2)
	 })
	 startimer=0
	end

 
 for s in all(starfield) do
  s.y+=s.speed
  if s.y>128 then
   del(stars,s)
  elseif s.speed<2 and s.speed>1 then
 	 s.c=6
 	elseif s.speed<=1 then
 	 s.c=5
 	end
 end
 
 for i=1,4 do
	 add(parts,{
	  x=58+rnd(12),
	  y=86+rnd(5),
	  r=2+rnd(2),
	  c=10,
	  speed=1+rnd(2)
	 })
 end

 for p in all(parts) do
  p.y+=p.speed
  p.r-=.1
  if p.r<0 then
   del(parts,p)
  end
  if p.r>3 then
   p.c=10
  elseif p.r<=3 and p.r>1 then
   p.c=9
  elseif p.r<=1 then
   p.c=5
  end
 end
 
 if btnp(➡️) then
  msgindex+=1
 end
end

function dcutscene()
 cls()
 for p in all(parts) do
  circfill(p.x,p.y,p.r,p.c)
 end
 
 for s in all(starfield) do
  pset(s.x,s.y,s.c)
 end
 
 spr(128,48,56,4,4)
 
 rectfill(10,90,118,110,7)
 rect(9,89,119,111,1)
 if csnum==1 then
  print(text1[msgindex],11,91,1)
  if msgindex>#text1 then
   state="mg1"
   msgindex=1
  end
 elseif csnum==2 then
  print(text2[msgindex],11,91,1)
  if msgindex>#text2 then
   mg2reset()
   msgindex=1
  end
 end
 print("➡️",111,105)
 
end
