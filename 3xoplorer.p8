--cutscene functions
function icutscene()
 parts={}
 
end

function ucutscene()
 for i=1,4 do
	 add(parts,{
	  x=58+rnd(12),
	  y=86+rnd(5),
	  r=2+rnd(2),
	  c=10,
	  speed=1+rnd(2)
	 })
 end

 for p in all(parts) do
  p.y+=p.speed
  p.r-=.1
  if p.r<0 then
   del(parts,p)
  end
  if p.r>3 then
   p.c=10
  elseif p.r<=3 and p.r>1 then
   p.c=9
  elseif p.r<=1 then
   p.c=5
  end
 end
end

function dcutscene()
 cls()
 for p in all(parts) do
  circfill(p.x,p.y,p.r,p.c)
 end
 spr(128,48,56,4,4)
end
