--player functions
function iplayer()
 player={
  x=10,
  y=110,
  spd=1,
  sprite=72
 }
 
 sprcounter=0
 spranim=0
end

function uplayer()
 local ptx=(player.x+8)/8
 local pty=(player.y+8)/8
 

 
 if btn(‚¨ÜÔ∏è) or btn(‚¨áÔ∏è) or btn(‚û°Ô∏è) or btn(‚¨ÖÔ∏è) then
  sprcounter+=.1
  if flr(sprcounter)%2==0 then
   spranim=0
  else spranim=1 end
 end
 if btn(‚¨ÜÔ∏è) then
  player.y-=player.spd
  if fget(mget(ptx,pty),4) then
   player.y+=player.spd
  end
  if spranim==1 then
  	player.sprite=76
  else player.sprite=78 end
 end
 if btn(‚¨áÔ∏è) then
  player.y+=player.spd
  if spranim==1 then
   player.sprite=72
  else player.sprite=74 end
 end
 if btn(‚û°Ô∏è) then
  player.x+=player.spd
  if spranim==1 then
   player.sprite=64
  else player.sprite=66 end
 end
 if btn(‚¨ÖÔ∏è) then
  player.x-=player.spd
  if spranim==1 then
 	 player.sprite=68
 	else player.sprite=70 end
 end

 if fget(mget(ptx,pty),0) then
  if btnp(üÖæÔ∏è) then
   state="selectionbox"
  end
 end

 if fget(mget(ptx,pty),3) then
  if btnp(üÖæÔ∏è) then
   state="textbox"
  end
 end

end

function dplayer()
 spr(player.sprite,player.x,player.y,2,2)
end
