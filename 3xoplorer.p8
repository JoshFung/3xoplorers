--selection box functions

function isel()
 seltable={"yes","no"}
 selected=1
end

function usel()
 if btnp(‚û°Ô∏è) then
  if selected==1 then
   selected+=1
  end
 elseif btnp(‚¨ÖÔ∏è) then
  if selected==2 then
   selected-=1
  end
 end
 
 if (selected==1 and btnp(‚ùé)) then
  state="cutscene"
 elseif (selected==2 and btnp(‚ùé)) then
  state="play"
 end
end

function dsel()
 rectfill(30,60,98,92,7)
	rect(30,60,98,92,1)
	rectfill(24+22*selected,78,40+22*selected,86,13)
	for i=1,#seltable do
	 print(seltable[i],27+22*i,80,1)
	end
	print("start minigame?",35,68,1)
 print("‚ùé",110,106,5)
end
