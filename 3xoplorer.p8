--flags
-- 0=minigame initiator
-- 3=textbox
-- 4=wall

function _init()
 state="title"
 
 title_timer=0
 title_color=1
 
 iplayer()
 isel()
 icutscene()
end

function _update()
	button_pressed = false
 --title screen
 if state=="title" then
  if (btnp(‚ùé) or btnp(üÖæÔ∏è)) then
   state="controls"
  end
  title_timer+=1
     if title_timer>2 then
      title_color+=1
      title_timer=0
     end
     if title_color>14 then
      title_color=1
     end
 end
 
 --play
 if state=="controls" then
 	ucontrols()
 end
 
 if state=="play" then
  uplayer()
 end
 
 if state=="textbox" then
  utxt()
 end
 
 if state=="selectionbox" then
  usel()
 end
 
 if state=="cutscene" then
  ucutscene()
 end
end

function _draw()
 if state=="play" then
  cls(0)
  map()
  dplayer()
 elseif state=="title" then
  cls(5)
  print("exoplorer",45,50,title_color)
        print("press z/x to start",27,65,13)
	elseif state=="controls" then
		dcontrols()
 elseif state=="minigame" then
  cls(5)
  print("minigame screen",35,50,7)
 elseif state=="textbox" then
  dtxt()
 elseif state=="selectionbox" then
  dsel()
 elseif state=="cutscene" then
  dcutscene()
 end
 
end
